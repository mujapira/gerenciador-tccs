generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL_MONGO")
}

type ComunidadeCriador {
  id   String @db.ObjectId
  tipo String
}

type ComunidadePosts {
  autor         ComunidadePostsAutor
  conteudo      String
  data_postagem DateTime             @db.Date
}

type ComunidadePostsAutor {
  id   String @db.ObjectId
  tipo String
}

type ComunidadeSeguidores {
  data_seguimento DateTime                     @db.Date
  seguidor        ComunidadeSeguidoresSeguidor
}

type ComunidadeSeguidoresSeguidor {
  id   String @db.ObjectId
  tipo String
}

type TccAluno {
  id   TccAlunoId @map("_id")
  nome String
}

type TccAlunoId {
  id              String   @map("_id") @db.ObjectId
  caminho_foto    String
  cidade          String
  cpf             String
  data_ingresso   DateTime @db.Date
  data_nascimento DateTime @db.Date
  email           String
  endereco        String
  estado          String
  matricula       String
  nome            String
  semestre_atual  Int
  telefone        String
  turma_id        String   @db.ObjectId
  turma_nome      String
}

type TccAvaliacoes {
  data_avaliacao DateTime                @db.Date
  descricao      String
  nota           Float
  numero         Int
  orientador     TccAvaliacoesOrientador
}

type TccAvaliacoesOrientador {
  id   String @map("_id") @db.ObjectId
  nome String
}

type TccClassificacao {
  id        String @map("_id") @db.ObjectId
  descricao String
}

type TccDocumentos {
  caminho    String
  data_envio DateTime @db.Date
  formato    String
  nome       String
  tamanho    Int
  tipo       String
}

type TccOrientador {
  id   String @map("_id") @db.ObjectId
  nome String
}

type TccPalavrasChave {
  id      String @map("_id") @db.ObjectId
  palavra String
}

type TccTema {
  id        String @map("_id") @db.ObjectId
  descricao String
}

type TurmaAlunos {
  id   String @map("_id") @db.ObjectId
  nome String
}

model a {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model aluno {
  id              String   @id @default(auto()) @map("_id") @db.ObjectId
  caminho_foto    String
  cidade          String
  cpf             String
  data_ingresso   DateTime @db.Date
  data_nascimento DateTime @db.Date
  email           String
  endereco        String
  estado          String
  matricula       String
  nome            String
  semestre_atual  Int
  telefone        String
  turma_id        String   @db.ObjectId
  turma_nome      String
}

model comunidade {
  id           String                 @id @default(auto()) @map("_id") @db.ObjectId
  criador      ComunidadeCriador
  data_criacao DateTime               @db.Date
  descricao    String
  imagem_capa  String
  nome         String
  posts        ComunidadePosts[]
  seguidores   ComunidadeSeguidores[]
}

model orientador {
  id               String @id @default(auto()) @map("_id") @db.ObjectId
  cpf              String
  departamento     String
  email            String
  nome             String
  telefone         String
  titulo_academico String
}

model tcc {
  id             String             @id @default(auto()) @map("_id") @db.ObjectId
  aluno          TccAluno
  avaliacoes     TccAvaliacoes[]
  classificacao  TccClassificacao
  documentos     TccDocumentos[]
  notaFinal      Float
  orientador     TccOrientador
  palavras_chave TccPalavrasChave[]
  status         String
  tema           TccTema
  titulo         String
}

model tcc_classificacao {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  descricao String
}

model tcc_estado {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  descricao String
}

model tcc_palavra_chave {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  palavra String
}

model tcc_tema {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  descricao String
}

model tcc_tipo_documento {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  descricao String
}

model turma {
  id     String        @id @default(auto()) @map("_id") @db.ObjectId
  alunos TurmaAlunos[]
  nome   String
}
